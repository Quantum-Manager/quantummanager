			   max="100" />

		<field name="sharpen"
			   type="number"
			   label="COM_QUANTUMMANAGER_CONFIG_IMAGE_SHARPEN_LABEL"
			   description="COM_QUANTUMMANAGER_CONFIG_IMAGE_SHARPEN_DESC"
			   default="0"
			   min="0"
